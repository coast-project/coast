Import('baseEnv')
import SomeUtils
from SomeUtils import *

progEnv = baseEnv.Clone()
libEnv = baseEnv.Clone()

packagename = 'compress'
libEnv.Tool(packagename + 'Lib', depsOnly=1)
compressLib = libEnv.SharedLibrary(packagename, listFiles(['*.cpp']))
progEnv.Tool('registerObjects', package=packagename, libraries=[compressLib])


{
	/simpleDNS2IPTest {
		/localhost {
			/name	!Defaults.any?LocalHostName
			/ip		!Defaults.any?LocalHostIp
		}
		/thisHost {
			/name	!Defaults.any?HostName
			/ip		!Defaults.any?HostNameIp
		}
		/thisHostFqdn {
			/name	!Defaults.any?FullQualifiedDomainName
			/ip		!Defaults.any?HostNameIp
		}
		/resolvableHost {
			/name	!Defaults.any?ResolvableHostFqdn
			/ip		!Defaults.any?ResolvableHostIp
		}
		/noHostToBeFound {
			/name	!Defaults.any?FaultyFqdn
			/ip		!Defaults.any?LocalHostIp
		}
	}
	/simpleIP2DNSTest {
		/localhost {
			/name	!Defaults.any?LocalHostName
			/fqdn	!Defaults.any?FullQualifiedDomainName
			/ip		!Defaults.any?LocalHostIp
		}
		/thisHost {
			/name	!Defaults.any?FullQualifiedDomainName
			/ip		!Defaults.any?HostNameIp
		}
	}
}

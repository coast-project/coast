	/Server {
		TestServer
	}
	/Servers {
		/Server	{
			/TestServer {
				AnotherServerWithoutLogConfigButShouldUseChannelsOfTestServer
			}
		}
	}

	/Renderers {
		/ContextLookupRenderer	{ Lookup }
	}

	/Actions {
		/LoggingAction	{ Log }
		/TimeLoggingAction	{ TimeLog }
	}

	/AppLogModule {
		/LogDir		"config"
		/Servers {
			/TestServer	{
				/AccessLog	{
					/FileName	"Access.log"
					/Format		%AccessLogFormat
					/Header		"TestHeader"
				}
				/ErrorLog	{
					/FileName	"Error.log"
					/Format		%ErrorLogFormat
					/Header		"ErrorlogTestHeader"
				}
				/AccessLog2	{
					/FileName	"Access2.log"
					/Format		%AccessLogFormat
					/Header		"Access2Header"
				}
				/ErrorLog2	{
					/FileName	"Error2.log"
					/Format		%ErrorLogFormat
					/Header		"Errorlog2TestHeader"
				}
				/TimeLog1 {
					/FileName	"MethodTime.log"
					/Format		%MethodTimeFormat
					/Header		"TimeLogTestHeader"
				}
				/NoHeaderLog
				{
					/FileName	"NoHeader.log"
					/Format		%NoHeaderLogFormat
					/SuppressEmptyLines	0
				}
				/EmptyHeaderLog
				{
					/FileName	"EmptyHeader.log"
					/Format		%EmptyHeaderLogFormat
					/Header		""
					/SuppressEmptyLines	1
				}
				/RelativeLogDir	{
					/LogDir		"."
					/RotateDir	"config"
					/FileName	"Relative.log"
					/Format		"Nothing"
					/Header		"# Relative-File"
				}
				/AbsoluteLogDir	{
					/LogDir		"/tmp"
					/RotateDir	"/tmp"
					/FileName	"Absolute.log"
					/Format		"Nothing"
					/Header		"# Absolute-File"
				}
			}
			/AnotherServerWithoutLogConfigButShouldUseChannelsOfTestServer {}
		}
	}
	/AccessLogFormat {
		{ /Lookup	TestMsg }
		" - Test"
	}

	/ErrorLogFormat {
		{ /Lookup	ErrorMsg }
		" - Test"
	}
	/MethodTimeFormat {
	  "<"
	  { /Lookup TimeLogEntry.Key }
	  ">: "
	  { /Lookup TimeLogEntry.Msg }
	  "->"
	  { /Lookup TimeLogEntry.Time }
	  " ms"
	}
	/NoHeaderLogFormat
	{
		{ /Lookup	TestMsg }
	}
	/EmptyHeaderLogFormat
	{
		{ /Lookup	TestMsg }
	}
	/AccessTestAction {
		/LoggingAction	{
			/Channel	AccessLog2
		}
	}
	/ErrorlogTestAction {
		/Log	{
			/Channel	ErrorLog2
		}
	}
	/NoChannelTestAction {
		/Log	{
		}
	}

	/TimeLoggingModule {
		/DoTiming	1
		/DoLogging	1
	}

	/TimeLogTestAction {
		/TimeLog	{
		  /Channel TimeLog1
		  /TimeEntries "Log.Times"
		}
	}
	#--- time logging tables ---

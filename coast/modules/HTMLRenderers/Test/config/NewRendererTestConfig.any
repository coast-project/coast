	/DLL {}
	/Modules {
		PagesModule
		RolesModule
		RenderersModule
	}
	/Pages {
		/Page	{ TestPage }
	}
	/Roles {
		/Role	{ TestRole }
	}
	/Renderers {
		/ContextLookupRenderer { Lookup }
	}

######## Test specific section ########

	/EnvForAllCases {
	}

	/RunOnly {
	}

	/TestCases {
		/TestName {
			/Env {
			}
			/Renderer {
				"234"
			}
			/Expected "234"
		}
		/MultiFuncBoxHeaderTest {
			/TmpStore {
				/BoxInputData			!"OptionBoxTestConfig.any?TestData"
				/BoxColumns				!"OptionBoxTestConfig.any?MultifunctionListBoxRendererTest.List"
				/myRenderer {
					"<form name=test>"
					/MultifunctionListBoxRenderer {
						/Name 			MyBox
						/Size			10
						/ColList		BoxColumns
						/ListName		BoxInputData
						/ValueRenderer { /Lookup BoxColumns.Value }
						/EditableList	1
					}
					"</form>"
				}
			}
			/Renderer {
				/ContextLookupRenderer myRenderer
			}
			/Expected	!"OptionBoxTestConfig.any?MultifunctionListBoxRendererTestOutput"
		}
	}
}

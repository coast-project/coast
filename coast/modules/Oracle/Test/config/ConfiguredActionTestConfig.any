{
	/DLL {
		CoastAppLog
		CoastDataAccess
		CoastRenderers
	}
	/Modules {
		ActionsModule
		RenderersModule
		OracleModule
		DataAccessImplsModule
		MappersModule
		AppLogModule
		ServersModule
	}

	/Servers {}
	# uncomment the server slot for enabling TimeLogging
	/Server	Server

	/Actions {
		/CallDataAccessAction	{ CallDA }
	}
	/Renderers {
		/SwitchRenderer 		{ Switch }
		/ContextLookupRenderer 	{ Lookup }
	}
	/OracleModule {
		/ConnectionPool {
			/ParallelQueries		4
			/CloseConnectionTimeout	20
		}
	}

	/DataAccessImpls {
		/OracleDAImpl {
			/SifsCoastOrcl {
				SelectEmployees
				SelectEmployeesTitlesOnce
			}
		}
	}

	/Mappers {
		/Input {
			/ParameterMapper {
				/SifsCoastOrcl {
					/SelectEmployees {
						SelectEmployeesTitlesOnce
					}
				}
			}
		}
		/Output {
			/ResultMapper {
				PutResultMapper
				/OracleDAImpl {
					SelectEmployees
					SelectEmployeesTitlesOnce
				}
			}
		}
	}


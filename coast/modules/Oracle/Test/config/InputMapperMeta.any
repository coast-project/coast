		/Name		"getempsalary"
		/Params {
			/ID		7839
		}
	}
	/SPSlotTest {
		/Name	ourSPName
		/Params {
			/P1		first
		}
	}
	/SPRenderedParamTest {
		/Params {
			/P1		{ /LookupMapper * }
		}
	}
	/SPExecute {
		/Name	{ /RendererMapper { /ContextLookupRenderer SPName } }
		/Params {
		}
	}

{

	/mybs {
		/Key "A Brain Dead Key"
	}
	/mybs0 {
		/KeyLoc "keyfile.any"
	}
	/mybc {
		/Key "My simple and non random key"
		# Initialization vector of 2 x 32 bits, factually extends the key
		# of a BlowfishCBCScrambler
		/InitVec0 0x01234567
		/InitVec1 0x089ABCDE
	}
	/mysign {
		/Key "A key for signing"
	}
}

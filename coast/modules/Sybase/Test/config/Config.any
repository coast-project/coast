{

#################################################################################
#
#	Module List definition
#	List of modules that are getting configured during GlobalInit and ReInit
#
#################################################################################
	/DLL {
		"libdataaccess.so"
	}

	/Modules {
		DataAccessImplsModule
		MappersModule
		ActionsModule
	}

	/Actions {
		/CallDataAccessAction	{ CallDA }
	}

	/DataAccessImpls {
		/SybDBDAImpl {
			/SybLocalSa {
				SybShowDatabases
				SybDeleteFromDatabase
			}
			/SybLocalTest {
				SybSearchTest
				SybInsertDatabases
				SybTestTypeConversions
				SQLErrorTest
			}
		}
	}

	/Mappers {
		/Input {
			/Mapper {
				SybSearchTest
				SybShowDatabases
				SybInsertDatabases
				SybDeleteFromDatabase
				SybTestTypeConversions
				SQLErrorTest
			}
		}
		/Output {
			/NameUsingOutputMapper {
				SybSearchTest
				SybShowDatabases
				SybInsertDatabases
				SybDeleteFromDatabase
				SybTestTypeConversions
				SQLErrorTest
			}
		}
	}
}

			/RendererMapper {
				"DELETE FROM pubs2..au_pix where au_id='cirp'"
			}
		}
	}

	/SybTestTypeConversions {
		/SQL	{
			"set rowcount 1 SELECT * FROM pubs2.guest.TestTypeTable"
		}
	}


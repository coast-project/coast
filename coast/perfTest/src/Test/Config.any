{

	/Application TestRunnerApplication

	/DLL {
		security
		stddataaccess
		actions
		perftesttest
	}

	/StressServer6666 {
		/Port	6666
	}

	/Modules {
		SecurityModule
		ActionsModule
		DataAccessImplsModule
		RolesModule
		PagesModule
		CacheHandlerModule
		MappersModule
		AcceptorFactoriesModule
		StressersModule
		FlowControllersModule
	}

	/AcceptorFactories {
		/AcceptorFactory {
			StressServer6666
		}
	}

	/Stressers {
		/DummyStresser	{
			Dummy1 Dummy2 Dummy3 Dummy4
		}
		/DataAccessStresser {
			Connection1
			Connection2
		}
		/ThreadedStresserRunner {
			TStresserRunner
			MultiRemote
		}
		/FlowControlDAStresser {
			FlowControlled
			FlowControlledDataAccessFailed
		}
		/RemoteStresser {
			RemoteStresser1
		}
	}

	/FlowControllers {
		/FlowController {
			ItopiaHomeFlow
		}
		/HttpFlowController {
			AnalyzeTestHTTPFlowController
			PrepareTestHTTPFlowController
			DataAccessFailsFlow
		}
	}

	/Pages {
		/Page	{ TestPage }
	}
	/Roles {
		/Role	{ TestRole }
	}
	/DefaultRole TestRole

#--- Transactions related stuff -------------------------------------------------------

#--- TRX Implementation objects ------------------------

	/DataAccessImpls {
		/HTTPDAImpl {
			AWebServer
			ItopiaCh
			HostUnknown
			AStressServer
			GetResult
		}
		/HTTPFileLoader {
			GetHTMLTestFile
			GetTreeMappedHTMLFile
		}
	}

	/Mappers {
		/Input {
			/ParameterMapper {
				GetResult
				AWebServer
				ItopiaCh
				HostUnknown
				GetHTMLTestFile
				GetTreeMappedHTMLFile
			}
			/AnythingToStreamMapper {
				AStressServer
			}
		}
		/Output {
			/ResultMapper {
				#GetResult
				AWebServer
				ItopiaCh
			}
			/StreamToAnythingMapper {
				AStressServer
			}
			/HTMLTreeAndSortedTagsMapper {
				GetHTMLTestFile
				GetResultTree
			}
			/HTMLTreeMapper {
				GetTreeMappedHTMLFile
			}
			/SortedTagsHTMLMapper {
				GetResult
				HostUnknown
			}
		}
	}

	/Actions {
		/CallDataAccessAction			{ CallDA }
		/FlowControllerPerformAnalyzeAction			{ PerformAnalyze }
		/FlowControllerPrepareRequestAction			{ PrepareRequest }
	}
}

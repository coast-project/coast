Import('baseEnv')
import SomeUtils, pdb
from SomeUtils import *

progEnv = baseEnv.Clone()
libEnv = baseEnv.Clone()

packagename = 'PerfTestTest'
libEnv.Tool(packagename + 'Lib', depsOnly=1)
createdLib = libEnv.SharedLibrary(packagename, listFiles(['*.cpp']))

Import('wdapp')

progEnv.Tool('registerObjects', package=packagename,
             libraries=[createdLib],
             requiresLibs=['CoastStdDataAccess', 'CoastActions', 'CoastRenderers', 'CoastSecurity'],
             testApps=[wdapp],
             config=listFiles(['*.txt', '*.html', '*.any']))

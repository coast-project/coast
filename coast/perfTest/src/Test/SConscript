Import('baseEnv')
import SomeUtils
from SomeUtils import *

progEnv = baseEnv.Clone()
libEnv = baseEnv.Clone()

packagename = 'PerfTestTest'
libEnv.Tool(packagename + 'Lib', depsOnly=1)
createdLib = libEnv.SharedLibrary(packagename, listFiles(['*.cpp']))
progEnv.Tool('CoastStdDataAccessLib')
progEnv.Tool('CoastActionsLib')
progEnv.Tool('CoastRenderersLib')
progEnv.Tool('CoastSecurityLib')
progEnv.Tool('registerObjects', package=packagename, libraries=[createdLib])


Import('baseEnv')
import SomeUtils
from SomeUtils import *

progEnv = baseEnv.Clone()

targetname = 'wdapp'
progEnv.Tool('wdbaseLib')
createdProgram = progEnv.Program(targetname, listFiles(['Main.cpp']))
progEnv.Tool('registerObjects', package=targetname, binaries=[createdProgram])


import StanfordUtils

progEnv = StanfordUtils.CloneBaseEnv()
libEnv = StanfordUtils.CloneBaseEnv()

packagename = 'testfw'
StanfordUtils.DependsOn(libEnv, packagename, depsOnly=1)
libEnv['_NONLAZYLINKFLAGS'] = ''
createdLib = libEnv.SharedLibrary('testfw', StanfordUtils.listFiles(['*.cpp']))
progEnv.Tool('registerObjects', package=packagename, libraries=[createdLib])


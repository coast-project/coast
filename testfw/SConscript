Import('baseEnv')
import SomeUtils
from SomeUtils import *

progEnv = baseEnv.Clone()
libEnv = baseEnv.Clone()

packagename = 'testfw'
libEnv.Tool(packagename + 'Lib', depsOnly=1)
testfwLib = libEnv.StaticLibrary('testfw', listFiles(['*.cpp']))
progEnv.Tool('registerObjects', package=packagename, libraries=[testfwLib])

